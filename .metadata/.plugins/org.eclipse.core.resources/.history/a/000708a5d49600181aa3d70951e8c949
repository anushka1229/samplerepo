
public class Demo1D {
	
	public static void main(String[] args)
	{
		int []numbers;
		numbers=new int[5];
		numbers[0]=100;
		numbers[1]=300;
		numbers[2]=220;
		numbers[3]=100;
		numbers[4]=456;
		numbers[5]=654;
		System.out.println(numbers[0]+numbers[1]+numbers[5]+numbers[2]+numbers[4]+numbers[3]);
	}
	

}
