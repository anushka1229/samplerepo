
public class Person {
  private int age;
  private String name;
  public Person() //default initialization
  {
	  name="Anushka";
	  age=24;
  }
  public void display()
  {
	  System.out.println("Name of person "+name+"\tAge->"+age);
  }
  public Person(int a, String n)
  {
	  age=a;
	  name=n;
  }
  public void setAge(int a)
  {
	  age=a;
  }

}
