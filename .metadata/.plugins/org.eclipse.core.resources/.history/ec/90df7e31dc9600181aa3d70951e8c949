
public class Demo1DObjects {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Person[] persons;
		persons=new Person[4];
		persons[0]=new Person();
		persons[1]=new Person(12,"anushka");
		persons[3]=new Person(13,"shreya");
		persons[2]=new Person(23,"ruby");
		
		persons[2].display();
		persons[0].display();
		

	}

}
