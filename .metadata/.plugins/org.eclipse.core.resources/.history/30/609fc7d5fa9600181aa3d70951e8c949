		int search=54;
		int flag=0;
	    i=0;
		while(i<numbers.length)
		{
		  if(numbers[i]==search)
		  {
			  flag=1;
			   break;
		  }
		  i++;
		}
		if(flag=0)
		{
			System.out.println("not found");
		}
		else
		{
			System.out.println("found");
		}
		

	}

}
