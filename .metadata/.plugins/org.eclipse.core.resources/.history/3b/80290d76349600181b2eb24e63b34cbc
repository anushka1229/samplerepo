
public class Person {
  private int age;
  private String name;
  public Person() //default initialization
  {
	  name="Anushka";
	  age=24;
  }
  public void display()
  {
	  System.out.println("Name of person "+name+"\tAge->"+age);
  }
  public Person(int age, String name)
  {
	  age=this.age;
	  name=this.name;
  }
  public void setAge(int a)
  {
	  age=a;
  }

}
